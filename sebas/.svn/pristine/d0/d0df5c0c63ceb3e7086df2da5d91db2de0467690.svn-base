<ui:composition template="/template/template.xhtml"
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jstl/core">
	<ui:define name="corpo">
	
		<c:if test="#{sessao.modulosUsuario == null || sessao.modulosUsuario.size() == 0}">
			<!-- usuário sem módulos vinculados ao perfil -->
			<div class="ui-messages-error ui-corner-all" style="margin:10px;">
				<span class="ui-messages-error-icon"></span>
				<span class="ui-message-error-summary"><h:outputText value="#{texto.texto_para_usuario_sem_modulo_vinculado}" escape="false" /></span>
			</div>
		</c:if>

	</ui:define>
</ui:composition>